.PHONY: all clean check
OCAMLINCLUDES += ../src
USE_OCAMLFIND = true


FILES[] =
	lambda_test

OCAMLPACKS[] =
	oUnit

PROGRAM = test
OCAML_LIBS += ../src/target

clean:
	rm -f *~ *.opt *.cmi *.cmx *.o *.omc *.cma *.cmxa

.DEFAULT: all

all : $(OCamlProgram $(PROGRAM), $(FILES))

check : all
	./$(PROGRAM)
